@DROP TABLE DATASETS;
CREATE TABLE DATASETS  (
                    ID             NUMBER(10) NOT NULL PRIMARY KEY ,
                    NPer           NUMBER(4)  NOT NULL ,
                    TapeId         VARCHAR2(2)  ,
                    TapeNumb       NUMBER(4)    , 
                    FileNumb       NUMBER(3)    ,
                    TMSLocation    VARCHAR2(4)  ,
                    Details        VARCHAR2(80) ,
                    Datatype       VARCHAR2(4)  ,
                    ProcessLevel   VARCHAR2(4)  ,
                    ExistEdir      VARCHAR(1)   ,
                    Complete_Proc  VARCHAR(1)   ,
                    RawYear        NUMBER(4)    ,
                    FirstRun       NUMBER(10)   ,
                    LastRun        NUMBER(10)   ,
                    Date_Proc      NUMBER(8)    ,
                    JuliaVersion   NUMBER(3)    ,
                    MiniVersion    NUMBER(3)    ,
                    CopyFlag       NUMBER(1)    ,
                    SizeMbytes     NUMBER(4)    , 
                    IHomelab       NUMBER(2)    , 
                    Nevents        NUMBER(8)    )
                    storage (initial 7M next 2M  maxextents 5);

CREATE INDEX I_DATASETS_Processlevel on DATASETS (RawYear,Datatype,Ihomelab,
                        ProcessLevel,Date_Proc,JuliaVersion,MiniVersion,Complete_Proc)
       storage (initial 3M next 1M maxextents 5)
       tablespace indx01;

CREATE INDEX I_DATASETS_Fili on DATASETS (TapeId,TapeNumb,FileNumb,Details,FirstRun,ExistEdir,SizeMbytes,ID)
       storage (initial 3M next 1M maxextents 5)
       tablespace indx01;



DROP TABLE RUNSETS;
CREATE TABLE RUNSETS  (
                    ID             NUMBER(10) NOT NULL PRIMARY KEY ,
                    DatasetsID     NUMBER(10) NOT NULL ,
                    Run            NUMBER(6) NOT NULL ,           
                    LepFill       NUMBER(6)    )
                    storage (initial 6M next 2M maxextents 5);

CREATE UNIQUE INDEX I_RUNSETS_DATASETS on RUNSETS (DatasetsID,Run)
       storage (initial 10M next 3M maxextents 5)  
       tablespace indx01;



DROP TABLE RUNCOND;
CREATE TABLE RUNCOND  (
                    ID            NUMBER(10) NOT NULL PRIMARY KEY ,
                    RunNumb       NUMBER(6)  NOT NULL ,
                    NPer          NUMBER(4)  NOT NULL ,
                    RunSelection  VARCHAR2(32) ,
                    RawTape       VARCHAR2(6)  ,
                    LepFill       NUMBER(6)    ,
                    WritDate      NUMBER(8)    ,
                    WritHour      NUMBER(6)    ,
                    EVnumb        NUMBER(6)    ,
                    ExactEnergy   NUMBER(6)    ,
                    RoundedEnergy NUMBER(6)    ,
                    NumZ0         NUMBER(5)    ,
                    NumBh         NUMBER(5)    ,
                    CrosSec       NUMBER(6)    ,
                    LUmin         FLOAT        ,
                    VdetLumi      FLOAT        ) 
                    storage (initial 5M next 1M maxextents 5);

CREATE UNIQUE INDEX I_RUNCOND_RunNumb on RUNCOND (RunNumb)
       storage (initial 2M next 500k maxextents 5)
       tablespace indx01;



DROP TABLE RUNBYYEAR;
CREATE TABLE RUNBYYEAR  (
                    ID             NUMBER(10) NOT NULL PRIMARY KEY ,
                    LepCategory    VARCHAR2(8)  ,
                    Year           NUMBER(4)    ,
                    FirstRun       NUMBER(10)   ,
                    LastRun        NUMBER(10)   ,
                    FirstFill      NUMBER(6)    ,
                    LastFill       NUMBER(6)    ,
                    FirstNper      NUMBER(4)    ,
                    LastNper       NUMBER(4)    ,
                    WrunselPatt    VARCHAR2(30) )
                    storage (initial 12K next 3k maxextents 5);

 

DROP TABLE ENERGYLEP;
CREATE TABLE ENERGYLEP  (
                    ID             NUMBER(10) NOT NULL PRIMARY KEY ,
                    LepCategory    VARCHAR2(8)  ,  
                    Year           NUMBER(4)    ,
                    RoundedEnergy  NUMBER(6)    )
                    storage (initial 8k next 2k maxextents 5) ;
 


DROP TABLE RUNQUALS;
CREATE TABLE RUNQUALS  (
                    ID             NUMBER(10) NOT NULL PRIMARY KEY ,
                    Year           NUMBER(4)  NOT NULL ,
                    NumBitpattern  NUMBER(2)    ,
                    RunqualKeyword VARCHAR2(2)  ,
                    ExplainRunqual VARCHAR2(20) ) 
                    storage (initial 30k next 10k maxextents 5);

 

DROP TABLE DATASTAT;
CREATE TABLE DATASTAT  (
                    ID             NUMBER(10) NOT NULL PRIMARY KEY ,
                    Year           NUMBER(4)  NOT NULL ,
                    ProcessLevel   VARCHAR2(4)  ,
                    NumbRaw        NUMBER(6)    ,
                    NumbPot        NUMBER(6)    ,
                    NumbDst        NUMBER(6)    ,
                    NumbMini       NUMBER(6)    ,
                    MbytesRaw      NUMBER(8)    ,
                    MbytesPot      NUMBER(8)    ,
                    MbytesDst      NUMBER(8)    ,
                    MbytesMini     NUMBER(8)    ,
                    IHomelab       NUMBER(2)    ) 
                    storage (initial 30k next 10k maxextents 5) ; 
 


DROP TABLE HOMEDESC;
CREATE TABLE HOMEDESC  (
                    ID             NUMBER(10) NOT NULL PRIMARY KEY ,
                    IHomelab       NUMBER(10) NOT NULL ,
                    HomeName       VARCHAR2(12) )
                    storage (initial 8k next 2k maxextents 5); 
 


DROP TABLE CARTDES;
CREATE TABLE CARTDES  (
                    ID            NUMBER(10) NOT NULL PRIMARY KEY ,
                    TapeId        VARCHAR2(2)  ,
                    FirstNum      NUMBER(4)    ,
                    LastNum       NUMBER(4)    ,
                    TapeLabel     VARCHAR2(2)  )
                    storage (initial 8k next 2k maxextents 5);



DROP TABLE LUMI4BUNCH;
CREATE TABLE LUMI4BUNCH  (
                    ID            NUMBER(10) NOT NULL PRIMARY KEY ,
                    RunNumb       NUMBER(6)  NOT NULL ,
                    LumQual       NUMBER(4)    ,
                    Numbh1        NUMBER(5)    ,
                    Lumin1        FLOAT        ,
                    Numbh2        NUMBER(5)    ,
                    Lumin2        FLOAT        ,
                    Numbh3        NUMBER(5)    ,
                    Lumin3        FLOAT        ,
                    Numbh4        NUMBER(5)    ,
                    Lumin4        FLOAT        ) 
                    storage (initial 120k next 30k maxextents 5);



DROP TABLE MCSETS;
CREATE TABLE MCSETS  (
                    ID               NUMBER(10) NOT NULL PRIMARY KEY ,
                    KinCode          NUMBER(4)  NOT NULL ,
                    TapeId           VARCHAR2(2)  ,
                    TapeNumb         NUMBER(4)    , 
                    FileNumb         NUMBER(3)    ,
                    TMSLocation      VARCHAR2(4)  ,
                    Datatype         VARCHAR2(8)  ,
                    Details          VARCHAR2(80) ,
                    KingalKeyWord    VARCHAR2(60) ,
                    ExistEdir        VARCHAR2(1)  ,
                    GeometryYear     NUMBER(4)    ,
                    Date_Proc        NUMBER(8)    ,
                    ExactEnergy      NUMBER(6)    ,
                    RoundedEnergy    NUMBER(6)    ,
                    ParticleCode     NUMBER(3)    ,
                    GalephVersion    NUMBER(3)    ,
                    JuliaVersion     Number(3)    ,
                    MiniVersion      Number(3)    ,
                    ParticleMass     Number(6)    ,
                    SizeMbytes       Number(4)    ,
                    CopyFlag         Number(1)    ,
                    IHomelab         Number(2)    , 
                    Nevents          Number(8)    , 
                    ProdNumb         Number(2)    , 
                    RunNumb          Number(4)    , 
                    Ranmar1          Number(6)    , 
                    Ranmar2          Number(6)    , 
                    OpsysKin         VARCHAR2(4)  ,
                    OpsysGal         VARCHAR2(4)  ,
                    OpsysJul         VARCHAR2(4)  ,
                    OpsysMini        VARCHAR2(4)  )                
                    storage (initial 14M next 7M maxextents 20 pctincrease 0);

CREATE INDEX I_MCSETS_GeometryYear on MCSETS (GeometryYear)
       storage (initial 2M next 1M maxextents 10 pctincrease 0)
       tablespace indx01; 

CREATE INDEX I_MCSETS_Specialpart on MCSETS (GeometryYear,KinCode,Datatype,RoundedEnergy,IHomelab,ParticleCode)
       storage (initial 4M next 2M maxextents 20 pctincrease 0)
       tablespace indx01;

CREATE INDEX I_MCSETS_Mcdatasets on MCSETS (GeometryYear,Datatype,RoundedEnergy,IHomelab,KingalKeyWord)
       storage (initial 4M next 2M maxextents 20 pctincrease 0)
       tablespace indx01;




DROP TABLE MCSTAT;
CREATE TABLE MCSTAT  (
                    ID             NUMBER(10) NOT NULL PRIMARY KEY ,
                    GeometryYear   NUMBER(4)  NOT NULL ,
                    LepCategory    VARCHAR2(8) ,
                    KinCode        NUMBER(4)  NOT NULL ,
                    KinName        VARCHAR2(8)  ,
                    NumbPot        NUMBER(6)    ,
                    NumbMini       NUMBER(6)    ,
                    MbytesPot      NUMBER(8)    ,
                    MbytesMini     NUMBER(8)    ,  
                    IHomelab       NUMBER(2)    )  
                    storage (initial 15k next 3k maxextents 5);



DROP TABLE MCLEPENERGY;
CREATE TABLE MCLEPENERGY  (
                    ID             NUMBER(10) NOT NULL PRIMARY KEY ,
                    LepCategory    VARCHAR2(8) ,
                    GeometryYear   NUMBER(4)  NOT NULL ,
                    KinCode        NUMBER(4)  NOT NULL ,
                    KinName        VARCHAR2(8)  ,
                    RoundedEnergy  NUMBER(6)    )
                    storage (initial 10k next 3k maxextents 5);



DROP TABLE MCKEYSTAT;
CREATE TABLE MCKEYSTAT  (
                    ID             NUMBER(10) NOT NULL PRIMARY KEY ,
                    GeometryYear   NUMBER(4)  NOT NULL , 
                    KinCode        NUMBER(4)  NOT NULL ,
                    KinName        VARCHAR2(8)  ,
                    KingalKeyWord  VARCHAR2(60) ,
                    ExplainKeyword VARCHAR2(60) ,
                    ParticleMass   NUMBER(6)    ,
                    NumbKingal     NUMBER(6)    ,
                    NumbPot        NUMBER(6)    ,
                    NumbMini       NUMBER(6)    ,
                    IHomelab       NUMBER(2)    )
                    storage (initial 300k next 100k maxextents 5);

CREATE UNIQUE INDEX I_MCKEYSTAT_KinKey on MCKEYSTAT (KingalKeyWord,KinCode,
       GeometryYear,IHomelab)
       storage (initial 2M next 500k maxextents 5)
       tablespace indx01;



DROP TABLE MCPARTICLES;
CREATE TABLE MCPARTICLES  (
                    ID             NUMBER(10) NOT NULL PRIMARY KEY ,
                    ParticleName   VARCHAR2(12)  ,
                    ParticleCode   NUMBER(3)     ,
                    AntipartCode   NUMBER(3)     ) 
                    storage (initial 15k next 4k maxextents 5);

CREATE UNIQUE INDEX I_MCPARTICLES_ParticleCode on MCPARTICLES (ParticleCode)
       storage (initial 2M next 500k maxextents 5)
       tablespace indx01;








