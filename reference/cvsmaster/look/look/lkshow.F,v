text
@d1 1
a1 2
*DK lkshow
      SUBROUTINE LKSHOW
d3 1
d9 1
a9 1
      DATA IOLD/0/
d11 12
a22 3
      IF(IW(IW(3)-3).EQ.INTCHA('EVEH')) THEN
        IEVTR = IW(IW(3)+6)
        IRUNTR = IW(IW(3)+2)
d24 2
a25 2
      ELSEIF(IW(IW(3)-3).EQ.INTCHA('RUNR')) THEN
        IRUNTR = IW(IW(3)+2)
d27 1
a27 4
      ELSEIF(IW(IW(3)-3).EQ.INTCHA('RUNH')) THEN
        IRUNTR = IW(IW(3)+2)
        WRITE(LOUT,*)' RUN RECORD,    run:',IRUNTR
      ELSE
@


