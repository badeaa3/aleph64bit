1.4
log
@alephio 7.3 enforce -s 600 in alk7fil and not -S 600
@
text
@       SUBROUTINE ALK7FIL (TAPE,IRET)
C ------------------------------------------------------------------
C - F.Ranjard - 920114
CKEY ALEF RUN TAPE / USER
C! set the FILI CARD for a given K7
C - Input   : TAPE       / A  = tape number i.e. AA1234_99
C - Output  : IRET       / I  = return code
C                               =0  Ok
C                               =1  no K7
C                               =2  no space to create FILI card
C -------------------------------------------------------------------
#ifndef DOC
      CHARACTER*(*) TAPE
#include "bcs.h"
      INTEGER ACARD1
      CHARACTER STRING*68, TYPE*4, LABEL*3, CART*30, SIZE*7
C -------------------------------------------------------------------
      IER = 0
      IND = ACARD1(' ')
      IND = NDROP ('FILI',0)
      CART = TAPE
      CALL CLTOU (CART)
      IF (CART(1:1) .EQ. ' ') THEN
         IER = 1
         GOTO 999
      ENDIF
      IND = NBANK ('FILI',0,17)
      IF (IND.EQ.0) THEN
         IER = 2
      ELSE
C
         LC = LNBLNK(CART)
         IF (LC.EQ.6) CART = CART(1:6)//'.1'
         JU = INDEX (CART,'_')
         IF (JU.GT.0) CART(JU:JU) = '.'
         JP = INDEX(CART(8:),'.')
         IF (JP.EQ.0) THEN
            LABEL = '.SL'
            IF (CART(1:2).EQ.'AR' .OR. 
     &          CART(1:2).EQ.'AS' .OR.
     &          CART(1:2).EQ.'AU' .OR.
     &          CART(1:2).EQ.'AV' .OR.
     &          CART(1:2).EQ.'AT' .OR.
     &          CART(1:1).EQ.'Y' ) LABEL = '.AL'
            IF (CART(1:2).EQ.'AX') LABEL = '.NL'
            LC = LNBLNK(CART)
            CART = CART(1:LC)//LABEL
         ENDIF
         JS = INDEX(CART,'-S')
         IF (JS.EQ.0) THEN
            SIZE = ' -S 200'
            IF (CART(1:2).EQ.'AR' .OR. 
     &          CART(1:2).EQ.'AS' .OR.
     &          CART(1:2).EQ.'AU' .OR.
     &          CART(1:2).EQ.'AV' .OR.
     &          CART(1:2).EQ.'AT' .OR.
     &          CART(1:1).EQ.'Y') SIZE = ' -S 600'
            LC = LNBLNK(CART)
            CART = CART(1:LC)//SIZE 
         ENDIF
         JS = INDEX(CART,'-S')
         CART(JS+1:JS+1) = 's'
         LC = LNBLNK(CART)
         STRING = 'ALDATA | EPIO | CART '//CART(1:LC)
         CALL ALINST (STRING,IW(IND+1),NWRDS)
         IND = NBANK('FILI',0,NWRDS)
      ENDIF
C
 999  CONTINUE
      END
#endif
@


1.3
log
@alephio72 size param in alk7fil.F
@
text
@d61 2
@


1.2
log
@alephio 7.2 introduce -s parameter in alk7fil
@
text
@d57 1
a57 2
     &          CART(1:1).EQ.'Y') SIZE = ' -S 600'            LC = LNBLNK(CART)

@


