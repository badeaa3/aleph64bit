text
@      SUBROUTINE ANAREC(LUN,NREC)
      INTEGER IBUF(3000)
      REWIND LUN
      IREC=0
   10 READ(LUN,END=90) NTOT,(IBUF(I),I=1,NTOT)
      IREC=IREC+1
      CALL BBUFM(IBUF,1,NTOT,6)
      IF(IREC.LT.NREC) GOTO 10
   90 CONTINUE
  100 RETURN
      END
@


