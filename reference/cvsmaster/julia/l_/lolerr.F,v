@
text
@d45 1
a45 1
      DATA MAXPL,MINPL,MAXST,MINST,RMNPL/7500,-1000,60000,-4000,0.8/
d133 2
a134 1
          IF(IEMX.GT.MAXPL.AND.ISUM.NE.0) THEN
d183 3
a185 2
          IF(MEVST(J,MODU).LT.MINST) SPARK=.TRUE.
          IF(MEVST(J,MODU).GT.MAXST) SPARK=.TRUE.
d191 1
a191 1
        IW(KLOLE+LMHLEN+JLOLER) = -1
@


