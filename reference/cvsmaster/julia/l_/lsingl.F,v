@
text
@d6 2
d27 1
d32 1
d39 2
d58 1
a58 1
C Find highest energy cluster
a60 1
      EMAX = 0.
d62 1
d64 2
a65 2
        IF(RTABL(KLIDT,I,JLIDEC).GT.EMAX) THEN
          EMAX = RTABL(KLIDT,I,JLIDEC)
d69 1
a69 1
      IF(IDT.EQ.0)                                     GOTO 999
d79 1
a79 1
      RW(KLSI+JLSIEN) = EMAX
@


