 	frefit.F ftpcer.F ftrack.F
 	gambnk.F gapeco.F gasteer.F
 	hcalib.F hcalrd.F hcatow.F hcbhis.F hcfclu.F hchist.F hclcra.F
 	hcltst.F hclufi.F hcos.F hcsimm.F hdgdeb.F hdgfil.F hdgpot.F
 	hdgsum.F hdprec.F hdspre.F hflnoi.F hinijo.F hiniru.F hmener.F
 	hmfind.F hmroad.F hnoise.F hnoisy.F hpatco.F hphnoi.F hplink.F
 	hprana.F hpranp.F hprdig.F hpreda.F hprpot.F hprsum.F hprunc.F
 	hrcomp.F hrcpat.F hrdcal.F hroad.F hslink.F hslowc.F hspare.F
 	hsptre.F hstofi.F htrack.F htubfi.F htwcr.F
 	iasign.F iattma.F ibnkin.F icaswt.F icftoi.F iclimb.F icrcco.F
 	ideadw.F idigpr.F ienang.F ifind1.F ifind2.F ifitp.F iftrak.F
 	igetti.F igettp.F igicha.F iheler.F iinijo.F iiniru.F iintrk.F
 	imatch.F inigap.F insect.F intrak.F ipakdi.F ipdsti.F iphcor.F
 	ipreda.F iprsum.F ipstat.F irefin.F ireopd.F isave.F ishuff.F
 	ishufl.F islowc.F isrch.F itbook.F itccut.F itcopr.F itcrec.F
 	itcrep.F itctpc.F itctrk.F itcxng.F iterr.F itextn.F ithist.F
 	itidy1.F itidy2.F itlnk1.F itlnk2.F itpcan.F itrak.F itrecf.F
 	itreci.F itree.F itrkci.F itrkeq.F itrkzr.F itrmca.F
@
text
@d8 1
d15 2
a24 1
#include "lidtjj.h"
d31 2
a32 2
      PARAMETER( CTMIN=0.062,CTMAX=0.125,CCMIN=.22,
     &           CEMIN=30.0,CDMAX=0.004 )
a51 5
C Event weight
      W = 1.
      KKEVH = IW(NAMIND('KEVH'))
      IF(KKEVH.GT.0) W = RTABL(KKEVH,1,5)
C
a94 6
      KLIDT = IW(NALIDT)
      IF(KLIDT.LE.0)                                 GOTO 999
      NIDT = LROWS(KLIDT)
      IF(NIDT.LE.0)                                  GOTO 999
      KBHAB = IW(NABHAB)
      IF(KBHAB.LE.0)                                 GOTO 999
d96 3
a98 2
      KSFTR = IW(NAMIND('SFTR'))
      KLIFL = IW(NALIFL)
d105 3
a107 3
      DO 31 I=1,NIDT
        EC = RTABL(KLIDT,I,JLIDEC)
        IF(RTABL(KLIDT,I,JLIDTC).LT.PIBY2) THEN
d119 21
a139 1
      IF(IA.EQ.0.OR.IB.EQ.0)                          GOTO 999
d143 2
a144 2
         I1 = IB
         I2 = IA
d147 14
d163 6
a168 6
      E1  = RTABL(KLIDT,I1,JLIDEC)
      E2  = RTABL(KLIDT,I2,JLIDEC)
      TH1 = RTABL(KLIDT,I1,JLIDTC)
      PH1 = RTABL(KLIDT,I1,JLIDPC)
      TH2 = RTABL(KLIDT,I2,JLIDTC)
      PH2 = RTABL(KLIDT,I2,JLIDPC)
a187 2
      ICL1 = ITABL(KLIDT,I1,JLIDLO)
      ICL2 = ITABL(KLIDT,I2,JLIDLO)
d189 3
a191 8
C? Histogram strongly selected e+e- for calibration
C
      IF(E1.GT.CEMIN.AND.E2.GT.CEMIN.AND.
     &   TH1.GT.CTMIN.AND.TH2.GT.CTMIN.AND.
     &   TH1.LT.CTMAX.AND.TH2.LT.CTMAX.AND.
     &   ABS(COS(PH1)).GT.CCMIN.AND.
     &   ABS(COS(PH2)).GT.CCMIN.AND.
     &   DPSI.LT.CDMAX) THEN
d194 33
a226 80
         MOD1 = ITABL(KLCLU,ICL1,JLCLMO)
         CALL HF1(LHID+10+MOD1,EPAD(MOD1),1.)
         MOD2 = ITABL(KLCLU,ICL2,JLCLMO)
         CALL HF1(LHID+10+MOD2,EPAD(MOD2),1.)
         DO 40 I=1,LROWS(KLPDA)
            IF(ITABL(KLPDA,I,JLPDMO).EQ.MOD1) IP1=I
            IF(ITABL(KLPDA,I,JLPDMO).EQ.MOD2) IP2=I
   40    CONTINUE
         DO 41 IP=1,29
            RL = 1.5 + FLOAT(IP-1)*0.5
            E = RTABL(KLPDA,IP1,1+IP)
            CALL HF1(LHID+14+MOD1,RL,E)
            E = RTABL(KLPDA,IP2,1+IP)
            CALL HF1(LHID+14+MOD2,RL,E)
   41    CONTINUE
         DO 42 IP=30,38
            RL = 15.5 + FLOAT(IP-30)
            E = RTABL(KLPDA,IP1,1+IP)/2.
            CALL HF1(LHID+14+MOD1,RL+0.5,E)
            CALL HF1(LHID+14+MOD1,RL+1.,E)
            E = RTABL(KLPDA,IP2,1+IP)/2.
            CALL HF1(LHID+14+MOD2,RL+0.5,E)
            CALL HF1(LHID+14+MOD2,RL+1.,E)
   42    CONTINUE
C
C Plot Track - Cluster
         ITR = ITABL(KLIDT,I1,JLIDST)
         IF(ITR.GT.0) THEN
            TTR = RTABL(KSFTR,ITR,JSFTTH)
            PTR = RTABL(KSFTR,ITR,JSFTPH)
            X0TR = RTABL(KSFTR,ITR,JSFTX0)
            Y0TR = RTABL(KSFTR,ITR,JSFTY0)
            XTR = X0TR + SIG*ZREF*TAN(TTR)*COS(PTR+SIG*ZREF*DPHBSR)
            YTR = Y0TR + SIG*ZREF*TAN(TTR)*SIN(PTR+SIG*ZREF*DPHBSR)
            CALL HF1(LHID+19,ABS(XTR)-ABS(X1),1.)
            CALL HF1(LHID+20,ABS(YTR)-ABS(Y1),1.)
         ENDIF
         ITR = ITABL(KLIDT,I2,JLIDST)
         IF(ITR.GT.0) THEN
            TTR = RTABL(KSFTR,ITR,JSFTTH)
            PTR = RTABL(KSFTR,ITR,JSFTPH)
            X0TR = RTABL(KSFTR,ITR,JSFTX0)
            Y0TR = RTABL(KSFTR,ITR,JSFTY0)
            XTR = X0TR - SIG*ZREF*TAN(TTR)*COS(PTR-SIG*ZREF*DPHBSR)
            YTR = Y0TR - SIG*ZREF*TAN(TTR)*SIN(PTR-SIG*ZREF*DPHBSR)
            CALL HF1(LHID+19,ABS(XTR)-ABS(X2),1.)
            CALL HF1(LHID+20,ABS(YTR)-ABS(Y2),1.)
         ENDIF
C
C Plot average X and Y of vertex
         XAV = (X1+X2)*0.5
         YAV = (Y1+Y2)*0.5
         CALL HFILL(LHID+31,XAV,0.,1.)
         CALL HFILL(LHID+32,YAV,0.,1.)
      ENDIF
C
C? Histogram default Bhabha selection for luminosity checks
C
      K2 = ITABL(KLIFL,I2,JLIFK2)
      K1 = ITABL(KLIFL,I1,JLIFK1)
      KL = ITABL(KLIFL,I2,JLIFKL)
      KH = ITABL(KLIFL,I1,JLIFKH)
C
      IF(KBHAB.LE.0)                                 GOTO 999
      DO 100 I=4,6
        LH = LHID + 40 + (I-1)*10
        IF(BTEST(KL,I-1).AND.BTEST(K1,I-1).AND.BTEST(K2,I-1).AND.
     &    DPHI.GT.RTABL(KBHAB,I,JBHADP)) CALL HF1(LH+1,E1,1.)
C
        IF(BTEST(KH,I-1).AND.BTEST(K1,I-1).AND.BTEST(K2,I-1).AND.
     &    DPHI.GT.RTABL(KBHAB,I,JBHADP)) CALL HF1(LH+2,E2,1.)
C
        IF(BTEST(KH,I-1).AND.BTEST(KL,I-1).AND.BTEST(K1,I-1).AND.
     &    BTEST(K2,I-1)) CALL HF1(LH+3,DPHI*180./PI,1.)
C
        IF(BTEST(KH,I-1).AND.BTEST(KL,I-1).AND.BTEST(K2,I-1).AND.
     &   BTEST(K1,I-1).AND.DPHI.GT.RTABL(KBHAB,I,JBHADP)) THEN
          CALL HF2(LH+4,ABS(X1),ABS(Y1),1.)
        ENDIF
  100 CONTINUE
@


1.1
log
@Initial revision
@
text
@a0 1
*DK lchist
@


1.1.1.1
log
@import julia 300
