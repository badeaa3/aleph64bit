 	hprana.F hpranp.F hprdig.F hpreda.F hprpot.F hprsum.F hprunc.F
 	hrcomp.F hrcpat.F hrdcal.F hroad.F hslink.F hslowc.F hspare.F
 	hsptre.F hstofi.F htrack.F htubfi.F htwcr.F
 	iasign.F iattma.F ibnkin.F icaswt.F icftoi.F iclimb.F icrcco.F
 	ideadw.F idigpr.F ienang.F ifind1.F ifind2.F ifitp.F iftrak.F
 	igetti.F igettp.F igicha.F iheler.F iinijo.F iiniru.F iintrk.F
 	imatch.F inigap.F insect.F intrak.F ipakdi.F ipdsti.F iphcor.F
 	ipreda.F iprsum.F ipstat.F irefin.F ireopd.F isave.F ishuff.F
 	ishufl.F islowc.F isrch.F itbook.F itccut.F itcopr.F itcrec.F
 	itcrep.F itctpc.F itctrk.F itcxng.F iterr.F itextn.F ithist.F
 	itidy1.F itidy2.F itlnk1.F itlnk2.F itpcan.F itrak.F itrecf.F
 	itreci.F itree.F itrkci.F itrkeq.F itrkzr.F itrmca.F
@
text
@d7 1
d21 23
a43 16
       CALL HBOOK1(LHID + 1,'LCAL- Pad energy$',60,0.,120.,0.)
       CALL HBOOK1(LHID + 2,'LCAL- Cluster energies$',60,0.,60.,0.)
       CALL HBOOK1(LHID + 3,'LCAL- Wire energy$',60,0.,120.,0.)
       CALL HBOOK1(LHID + 4,'LCAL- Storey1/Etot$',50,0.,1.,0.)
       CALL HBOOK1(LHID + 5,'LCAL- Storey3/Etot$',50,0.,1.,0.)
C
C Histogram strongly selected e+e- for calibration
       CALL HBOOK1(LHID + 11,'LCAL-e+e-  Pad energy m1$',
     *   60,0.,60.,0.)
       CALL HBOOK1(LHID + 12,'LCAL-e+e- Pad energy m2$',
     *   60,0.,60.,0.)
       CALL HBOOK1(LHID + 13,'LCAL-e+e- Pad energy m3$',
     *   60,0.,60.,0.)
       CALL HBOOK1(LHID + 14,'LCAL-e+e- Pad energy m4$',
     *   60,0.,60.,0.)
       CALL HBOOK1(LHID + 15,'LCAL-e+e- Energy pr 0.5 rl m1$',
d45 1
a45 1
       CALL HBOOK1(LHID + 16,'LCAL-e+e- Energy pr 0.5 rl m2$',
d47 1
a47 1
       CALL HBOOK1(LHID + 17,'LCAL-e+e- Energy pr 0.5 rl m3$',
d49 1
a49 1
       CALL HBOOK1(LHID + 18,'LCAL-e+e- Energy pr 0.5 rl m4$',
d51 11
a61 23
C
C Histogram cluster-track matching
       CALL HBOOK1(LHID + 19,'LCAL-e+e- X(track)-X(clus)$',
     *   40,-2.,2.,0.)
       CALL HBOOK1(LHID + 20,'LCAL-e+e- Y(track)-Y(clus)$',
     *   40,-2.,2.,0.)
C
C Average X and Y
       CALL HBOOK1(LHID + 31,'LCAL average X$',50,-5.,5.,0.)
       CALL HBOOK1(LHID + 32,'LCAL average Y$',50,-5.,5.,0.)
C
C Histogram Bhabha selections for luminosity checks
       DO 100 I=4,6
         LH = LHID + 40 + (I-1)*10
         CALL HBOOK1(LH+1,'LCAL-Bhabha selection- E1$'
     *    ,60,0.,60.,0.)
         CALL HBOOK1(LH+2,'LCAL-Bhabha selection- E2$'
     *    ,60,0.,60.,0.)
         CALL HBOOK1(LH+3,'LCAL-Bhabha selection- Dphi$',
     *     90,0.,180.,0.)
         CALL HBOOK2(LH+4,'LCAL-Bhabha selection- Y vs X$'
     *     ,25,0.,25.,25,0.,25.,0.)
  100  CONTINUE
@


