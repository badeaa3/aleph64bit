      REAL HCUTCM,LCUTCM,MXZOCC,MXPOCC,DECFAC,USNOIS,WSNOIS
      INTEGER CHNOFF,NCHANM,MXUNBD,MXSIGM,FNLSUM,IPHOFF
      LOGICAL MCEVNT,LR1990,LR1991,LR1992
      LOGICAL MKVDXY,MKVFPH,SWAPP9,LPHNEW,MKVHOT
