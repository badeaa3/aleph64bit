1.1
log
@Initial revision
@
text
@d7 2
d12 1
a12 7
C!    If PASS0 running mode of JULIA :
C!       Look for TDPV MUST EXISTS,
C!          if TLAS exist and too old or does not exists
C!             use TDPV ('RAW' option of TDPVEL)
C!          if TLAS OK use TLAS ( 'ONL' option of TDPVEL)
C!    If NOT PASS0 running mode of JULIA :
C!       use TDPV or TDFV ( 'RAW' option of TDPVEL)
a15 4
#ifndef DOC
#include "bcs.h"
#include "tdpvjj.h"
#include "tlasjj.h"
a19 1
#include "bmacro.h"
d21 1
a21 3
      KTLAS = IW(NAMIND('TLAS'))
      KTDPV = IW(NAMIND('TDPV'))
C
d23 9
a31 12
      IF ( FPASS0 ) THEN
C?   PASS0 :
C?   TDPV MUST exists and TLAS if exists must not be too old
        IF ( KTDPV.NE.0 ) THEN
          IF ( KTLAS.NE.0 ) THEN
            ELAPS = RTABL (KTLAS,1,JTLAET)
            IF ( ELAPS.LT.TMTLAS ) OPTION = 'ONL'
          ENDIF
          CALL TPDVEL ( OPTION, DVSA, DVSB, IER)
        ELSE
          CALL RERROR ('RGETDV', -1, ' NO TDPV bank found')
        ENDIF
d33 2
a34 11
C?   Standard running mode of JULIA :
C?   use TDPV if exists or TDFV
        CALL TPDVEL ( OPTION, DVSA, DVSB, IER)
      ENDIF
      VIPAS0(1,1) = DVSA (1)
      VIPAS0(2,1) = DVSA (2)
      VIPAS0(3,1) = DVSA (3)
      VIPAS0(1,2) = DVSB (1)
      VIPAS0(2,2) = DVSB (2)
      VIPAS0(3,2) = DVSB (3)
  999 RETURN
a35 1
#endif
@


