@
text
@d6 2
a7 1
C!   Author:  M Mermikides  6-10-86
d42 2
a43 2
      ARG = (D0*(2.*RHO-D0) - RADSQ)/(2.*RAD*(RHO-D0))
      IF (ABS(ARG).GT.1.0) THEN
d50 3
a52 3
         ARG = (RADSQ-D0*D0)/(4.*RHO*(RHO-D0))
         IF (ARG.LT.0.) THEN
           APSI = 0.
d56 2
a57 2
         IF (APSI.GT.1.0) APSI = 1.0
         SS = 2.*RHO*ASIN(APSI)
d63 1
a63 1
         PHI = PHI0 + PI - Q*ALPHA
d66 1
a66 1
         V(3,2) = PAR(5) + (TWOPI*RHO -SS)*TANL
@


