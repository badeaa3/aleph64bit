          WRITE(LOUTRL,83) I,RTKS,RUSC,RUUC,RBAD,NDEXTM(I),ATRM,ERR,STRM
   83     FORMAT(
     &      1X,I2,4(3X,F10.5),T60,3X,I6,4X,F10.5,' +- ',F7.5,5X,F7.5)
