      CHISQ8 = 0.
d22 1
a22 1
        G(I)=0.0
d24 1
a24 1
           GG(J,I) = 0.0
d40 1
a40 1
         DDDV(3) = 0.
d43 1
a43 1
            D2DDV2(K,J) = 0.0
d45 1
a45 1
         D2DDV2(1,1) = 1./S - DX*DX/SSS
d47 1
a47 1
         D2DDV2(2,2) = 1./S - DY*DY/SSS
d51 1
a51 1
            DZ = 0.
d53 1
a53 1
               DZDV(J)=0.0
d55 1
a55 1
                  D2ZDV2(K,J) = 0.0
d84 1
a84 1
            DELTA = 2.0*DIR*DASIN( DMIN1( T/(2.*RADI(I)),1.D0 ) )
d92 1
a92 1
            DXTDA = SOL*RADI(I)*(1. - DX*DX/SS) /S
d95 1
a95 1
            DYTDB = SOL*RADI(I)*(1. - DY*DY/SS) /S
