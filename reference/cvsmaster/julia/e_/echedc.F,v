      DIMENSION ECCAV(2,36),ECCAT(2,36),COLDGT(36)
d39 1
d43 2
a44 2
      IF ( KECCA.EQ.0.OR.NECCA.NE.NMOD) THEN
        DO 10 IL = 1, NMOD
d50 2
a51 2
        CALL UCOPY ( RW(KECCA+LMHLEN+1),ECCAV(1,1),NMOD*2)
        CALL UCOPY ( RW(KECCA+LMHLEN+1),ECCAT(1,1),NMOD*2)
d54 1
a54 1
        CALL UCOPY ( RW(KECHE+LMHLEN+1),ECCAV(1,1),NMOD*2)
d56 1
a56 1
      DO 51 IL =1,NMOD
d79 1
a79 1
      LENG = LMHLEN + NMOD*LECHEA
d85 1
a85 1
      IW ( KECHE + LMHROW ) = NMOD
@


