text
@d39 1
a39 1
      EXTERNAL VNELRM
d167 1
a167 1
C  Cable swapps
d169 3
a171 1
        JFLAG = ALGTDB(LRCONS,'VSWP',-IRUNRC)
d210 1
a210 1
C  VSWP bank.  Variations in gains, alignment, etc. are handled
@


1.2
log
@julia version 279
@
text
@a0 1
*DK viniru
@


1.1
log
@Initial revision
@
text
@d20 4
a25 2
#include "implicit.h"
#include "vdimpl.h"
d28 2
a29 2
#include "vdflgs.h"
#include "vtcejj.h"
a30 1
#include "rlunit.h"
d32 4
a35 1
#include "vdbmcr.h"
d39 2
a40 1
      INTEGER  ALGTDB, GTSTUP, NLINK
d44 4
a47 4
      INTEGER NEVTP,IND, IVS, JVPHO,JVRCN,JVTRP,JVTPA,JVTCE,JVDEM
      INTEGER  NAMIND
      INTEGER JVMRE,JVMPC,NRUNH,IVIEW
      CHARACTER*8 LIST
d50 1
a50 1
C  Zero the run monitor variables
d53 1
a53 1
      DO 1001 IVIEW=1,2
d58 4
a61 1
 1001 CONTINUE
d63 14
d78 1
a78 4
C  Flags for MC, real 1990,1991, 1992 data (hopefully the hardware
C  wont' change after 1992).
C  Flags for MC, real 1990,1991, 1992 data (hopefully the hardware
C  wont' change after 1992).
d80 13
a92 4
      MCEVNT = IW(NAMIND('AJOB')).NE.0
      LR1990 = .NOT.MCEVNT.AND.(IVS.EQ.2)
      LR1991 = .NOT.MCEVNT.AND.(IVS.EQ.3)
      LR1992 = .NOT.MCEVNT.AND.(IVS.GE.4)
d94 1
a94 1
C read nominal geometry from database
d96 1
a96 4
      CALL VRDDAF(LRCONS,IRUNRC,IND)
      IF (IND .EQ. 0)THEN
        CALL RERROR('VINIRU',2,'Failed to read geometry banks')
      END IF
d98 1
a98 2
C  Get the setup-code dependent information; Pulseheight packing,
C  module map information, and other reconstruction constants
d100 3
a102 7
      IND = ALGTDB(LRCONS,'VPHOVRCNVTRPVTPAVTCEVDEMVDSM',-IVS)
      JVPHO = IW(NAMIND('VPHO'))
      JVRCN = IW(NAMIND('VRCN'))
      JVTRP = IW(NAMIND('VTRP'))
      JVTPA = IW(NAMIND('VTPA'))
      JVTCE = IW(NAMIND('VTCE'))
      JVDEM = IW(NAMIND('VDEM'))
d104 1
a104 1
C  Unpack PH offset information
d106 4
a109 5
      IF(JVPHO .GT. 0)THEN
        IPHOFF = ITABL(JVPHO,1,1)
      ELSE
        IPHOFF = 1024
      END IF
d111 1
a111 1
C  Get the single strip noise (offline units!!!)
d113 4
a116 9
      IF(JVTCE .GT. 0)THEN
        USNOIS = RTABL(JVTCE,1,JVTCUN)
        WSNOIS = RTABL(JVTCE,1,JVTCWN)
        DECFAC = RTABL(JVTCE,1,JVTCDF)
      ELSE
        USNOIS = 34.0
        WSNOIS = 34.0
        DECFAC = 0.0
      END IF
d118 1
a118 1
C For real data, read cuts and maps from the database
d120 3
a122 4
      IF(.NOT.MCEVNT)THEN
        IND = ALGTDB(LRCONS,'VMREVMPC',-IVS)
        JVMRE = IW(NAMIND('VMRE'))
        JVMPC = IW(NAMIND('VMPC'))
d124 1
a124 2
C  For 1990 data, look for the VHOT bank in the database (for 1991, it's
C  on the run header).
d126 3
a128 5
        IF(LR1990)THEN
          LIST = 'VHOTVMGN'
        ELSE
          LIST = 'VMGN'
        END IF
d130 1
a130 1
C  Get the run-number dependent constants
d132 1
a132 5
        IND = ALGTDB(LRCONS,LIST,-IRUNRC)
        IF (IND .EQ. 0)THEN
          CALL RERROR('VINIRU',1,'Failed to read maps')
        ENDIF
      ENDIF
d137 1
d141 3
d145 1
a145 2
C  Determine run-number dependent changes.
C  First, correct for number of dummy pulses offset in early data
d147 1
a147 1
      IF(MCEVNT)THEN
d149 1
a149 1
C  No special flags
d151 8
a158 5
        SWAPP9 = .FALSE.
        LPHNEW = .FALSE.
        CHNOFF = 0
        DECFAC = 0.0
      ELSE IF (LR1990)THEN
d160 1
a160 3
C  Fix time-dependent bugs in ONLINE
C  First, correct the early data for the module 9-10 (address 8-9)
C  phi side swap.
d162 1
a162 5
        IF(IRUNRC .LT. 7495)THEN
          SWAPP9 = .TRUE.
        ELSE
          SWAPP9 = .FALSE.
        END IF
d164 1
a164 1
C  Next, get the strip offset right
d166 1
a166 5
        IF(IRUNRC .LT. 7770)THEN
          CHNOFF = 2
        ELSE
          CHNOFF = 1
        END IF
d168 1
a168 1
C  Pulse-height format
d170 1
a170 8
        IF(IRUNRC .LT. 7960)THEN
          LPHNEW = .FALSE.
        ELSE
          LPHNEW = .TRUE.
        END IF
      ELSE IF (LR1991.OR.LR1992)THEN
        LPHNEW = .FALSE.
        CHNOFF = 1
d172 1
a172 1
C  Fix cabling error in 1992
d174 9
a182 1
        SWAPP9 = LR1992.AND.IRUNRC.LT.14480
d184 1
a184 3
C  Kludge fix for timing problem early in 1992; this can't be done
C  by the database, since the timing constant is stored by setup
C  code, and this affects only a few runs.
d186 6
a191 2
        IF(LR1992.AND.IRUNRC.LE.14831)THEN
          DECFAC = .22
a192 1
      END IF
d194 1
a194 1
C  Unpack the mapping information
d196 3
a198 1
      CALL VDMPIN
d200 16
a215 1
C  Load the common VRECON
d217 1
a217 1
      CALL VDRCUN
d219 1
a219 1
C  Process hot channel information
d221 9
a229 3
      CALL VHOTIN
C
C  Initialize Beam Position
d231 1
a231 1
      CALL VBSINR
d233 2
@


1.1.1.1
log
@import julia 300
@
text
@@
