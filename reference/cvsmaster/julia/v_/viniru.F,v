d41 2
a42 3
      LOGICAL  ALLEP1
      INTEGER  ALGTDB,       GTSTUP,NAMIND,NBANK,VNELRM,VDGINI,VDYEAR
      EXTERNAL ALGTDB,ALLEP1,GTSTUP,NAMIND,NBANK,VNELRM,VDGINI,VDYEAR
d88 2
a89 1
        IF (IW(NAMIND('PVGG')).GT.0.AND..NOT.ALLEP1(IRUNRC)) 
