d38 3
d42 12
a53 1
      IF (KBOMC .EQ. 0) GOTO 999
d55 25
a79 3
      DO 100 I = 1, 29
        CURCOR(I) = FLOAT ( ITABL (KBOMC, 1, JBOMCU+I-1) )
  100 CONTINUE
d81 1
a81 1
      XCURFL = .TRUE.
