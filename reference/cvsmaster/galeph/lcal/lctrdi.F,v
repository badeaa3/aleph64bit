text
@*DK lctrdi
      SUBROUTINE LCTRDI
C--------------------------------------------------------------
C! Convert trigger signals to MeV
C. - J.Dines Hansen & P.Hansen - 860417
C. - Modified P.Hansen - 950202
C. - Modifies bank LTTR, fills bank LWTR
C. - Called by  LCDIGI                           from this .HLB
C. - Calls LCWTRI                                from this .HLB
C -----------------------------------------------
#ifndef DOC
#include "jqcom.h"
#include "lcnamc.h"
#include "lccomc.h"
#include "bmacro.h"
C -------------------------------------------------------------
      KINDX = IW(NALTTR)
      IF(KINDX .LE. 0)                   GOTO 999
      DO 120 ISEG = 1,LROWS(KINDX)
        KLL = KROW(KINDX,ISEG)
        DO 110 IST = 1,3
C? Convert bank LTTR to MeV
          IW(KLL+IST) = NINT(IW(KLL+IST)*CHTOE(IST))
  110   CONTINUE
  120 CONTINUE
C
C - Fill Wire Trigger bank 'LWTR'
      CALL LCWTRI
  999 RETURN
      END
#endif
@


1.1
log
@Initial revision
@
text
@d6 1
d10 1
a11 1
      SAVE
a16 1
C - Convert bank LTTR to MeV using a factor LCHTOE at each storey
a18 1
      KWS = IW(NALCWS)
a19 1
        IFB = 2-(ISEG-1)/12
d23 1
a23 5
          IW(KLL+IST) = IW(KLL+IST)*LCHTOE(IST)
C? Update summary bank
          IF(KWS.GT.0) THEN
             IW(KWS+LMHLEN+6+IFB) = IW(KWS+LMHLEN+6+IFB)+IW(KLL+IST)
          ENDIF
@


