1.2
log
@add #ifndef where it was missing and remove dummy routines
@
text
@*DK muince
      SUBROUTINE MUINCE(NSLOT,IPLANE,NSTRPF,NAST,NBUS,NSTRPB,IFLAG)
C-----------------------------------------------------------
C!  Transforms endcap slot and strip nb. to astros and bus nb.
C
C  Author :   A. Antonelli, F. Bossi 1 July 89
C.  -Called by MUDGTZ                  from this .HLB
C.  -Calls none
C.
C-----------------------------------------------------------
#ifndef DOC
#include "bcs.h"
#include "munamc.h"
#include "mug1pr.h"
#include "mjjpar.h"
C
      SAVE
       PARAMETER(NROW1=22)
C
#include "bmacro.h"
#include "muiwna.h"
C
C            Astros type and astros nb.
        ISLOT = NSLOT - 100
        NTPAS= ITABL(JMESG,ISLOT,JMESTA)
        NAST = ITABL(JMESG,ISLOT,JMESNA)
C                 Bus type 1= X1 =1   Nb in MUHT
C                          2= Y1 =2
C                          3= X2 =4
C                          4= Y2 =3
C                          5= X3 =1
C                          6= Y3 =2
C                          7= X4 =4
C                          8= Y4 =3
C.......
        NBUSTY= IPLANE+4
        IF(ISLOT.LE.4) NBUSTY = IPLANE
        IF(ISLOT.GE.9.AND.ISLOT.LE.12) NBUSTY = IPLANE
        IF(IPLANE.EQ.3) NBUSTY = NBUSTY + 1
        IF(IPLANE.EQ.4) NBUSTY = NBUSTY - 1
C
        IREM=MOD(NBUSTY,2)
        IF(IREM.EQ.0) THEN
C
           IRSL = MOD(ISLOT,2)
           IF(ISLOT.LE.8) THEN
              IF(IRSL.EQ.0) THEN
                 JMPPT = ITABL(JMESG,ISLOT,JMESK2)
                 NMAXY = ITABL(JMETG,JMPPT,JMETNY)
                 NSTRPF = - (NSTRPF - NMAXY) - 1
              ENDIF
           ELSE
              IF(IRSL.GT.0) THEN
                 JMPPT = ITABL(JMESG,ISLOT,JMESK2)
                 NMAXY = ITABL(JMETG,JMPPT,JMETNY)
                 NSTRPF = - (NSTRPF - NMAXY) - 1
              ENDIF
           ENDIF
        ENDIF
C
        NBUS=0
        NSTRPB=0
        IFLAG = 0
        INAS = 3*(NTPAS-1)
        DO 1 J=1,NROW1
        IFIND = ITABL(JMEBG,J,JMEBB1+INAS)
        IF(IFIND.NE.NBUSTY) GO TO 1
        LIN = ITABL(JMEBG,J,JMEBL1+INAS)
        LFIN= ITABL(JMEBG,J,JMEBU1+INAS)
        IF(NSTRPF.GT.LFIN) GO TO 1
        NBUS = J - 1
        NSTRPB = NSTRPF - LIN
        IFLAG = 1
        GO TO 3
 1      CONTINUE
 3      CONTINUE
        RETURN
        END
#endif
@


1.1
