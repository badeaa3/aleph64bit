text
@*DK aggatt
      SUBROUTINE AGGATT (DET,ISTUP)
C--------------------------------------------------------------------
C!  Setup detector attributes in the interactive graphic version
C - B.Bloch-Devaux 911202
C - Input :   - DET     / CHA*2  : detector initials
C -           - ISTUP   / I      : setup code
C.  -Called by GUINTI                  from this .HLB
C.  -Calls  GSATT                      from  GEANT3
C.          ALGTDB                     from ALEPHLIB
C--------------------------------------------------------------------
#ifndef DOC
      CHARACTER*(*) DET
      CHARACTER*4 CHAINT,NAME,NATT
      INTEGER ALGTDB
#include "iocom.h"
#include "bcs.h"
      PARAMETER(JATTVO=1,JATTAN=2,JATTAV=3,LATTRA=3)
#include "bmacro.h"
C
C ----------------------------------------------------------------------
C
C     Look for attribute bank
      IRET = ALGTDB (LRDBIO,DET(1:2)//'AT',ISTUP)
      JATT = IW(NAMIND(DET(1:2)//'AT'))
C     Set up volume attribute to its value
      IF (JATT.GT.0) THEN
         DO 10 IV =1,LROWS(JATT)
            NAME(1:4) = CHAINT(ITABL(JATT,IV,JATTVO))
            NATT(1:4) = CHAINT(ITABL(JATT,IV,JATTAN))
            IVA = ITABL(JATT,IV,JATTAV)
            CALL GSATT(NAME,NATT,IVA)
  10     CONTINUE
      ENDIF
      GOTO 999
C - end
C
  999 CONTINUE
      RETURN
      END
#endif
@


1.1.1.1
log
@import galeph 300
