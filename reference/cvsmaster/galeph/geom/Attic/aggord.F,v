text
@*DK aggord
      SUBROUTINE AGGORD (DET,ISTUP)
C--------------------------------------------------------------------
C!  Order detector volumes along selected axis
C - B.Bloch-Devaux 911202
C - Input :   - DET     / CHA*2  : detector initials
C -           - ISTUP   / I      : setup code
C.  -Called by AGXXXX                  from this .HLB
C.  -Calls  GSORD                      from  GEANT3
C.          ALGTDB                     from ALEPHLIB
C--------------------------------------------------------------------
#ifndef DOC
      CHARACTER*(*) DET
      CHARACTER*4 CHAINT,NAME
      INTEGER ALGTDB
#include "iocom.h"
#include "bcs.h"
      PARAMETER(JORDVO=1,JORDAN=2,LSORDA=2)
#include "bmacro.h"
C
C ----------------------------------------------------------------------
C     Look for volume ordering bank
      IRET = ALGTDB (LRDBIO,DET(1:2)//'OR',ISTUP)
      JORD = IW(NAMIND(DET(1:2)//'OR'))
C     Set up volume ordering along selected axis
      IF (JORD.GT.0) THEN
         DO 10 IV =1,LROWS(JORD)
            NAME(1:4) = CHAINT(ITABL(JORD,IV,JORDVO))
            IAX = ITABL(JORD,IV,JORDAN)
            CALL GSORD(NAME,IAX)
  10     CONTINUE
      ENDIF
      GOTO 999
C - end
C
  999 CONTINUE
      RETURN
      END
#endif
@


1.1.1.1
log
@import galeph 300
@
text
@@
