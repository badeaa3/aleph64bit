1.2
log
@aleph302_7 : corr in kalm
@
text
@      SUBROUTINE UF2SCA(CORE_FRACT, CORE_SIZE, TAIL_SIZE, IELOSS)
C!  Load new scattering description into the UFG2 bank
#ifndef DOC
#include "ufg2jj.h"
#include "bcs.h"
#include "bmacro.h"
      KUFG2 = IW(NAMIND('UFG2'))
      IF(KUFG2 .LE. 0)THEN
        CALL ALTELL('UF2SCA - No UFG2 bank to modify',2,'RETURN')
        RETURN
      ENDIF
      RW(KROW(KUFG2,1)+JUFGCF) = CORE_FRACT
      RW(KROW(KUFG2,1)+JUFGCW) = CORE_SIZE
      RW(KROW(KUFG2,1)+JUFGTW) = TAIL_SIZE
      IW(KROW(KUFG2,1)+JUFGDE) = IELOSS

      CALL UFREIN

      RETURN
      END
#endif
@


