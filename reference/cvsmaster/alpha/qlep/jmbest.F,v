@
text
@d6 2
a7 1
C  Called from QTRUTH
d24 2
a25 2
      DO 10 K=1,KNMTCH(ITK)
        IF (KSMTCH(ITK,K).GT.MXHT) THEN
d29 1
a29 1
10    CONTINUE
d38 1
a38 1
        DO 50 I = 1,KNMOTH(IMC)
d40 5
a44 3
          DO 50 J = 1,KNMTCH(MOM)
50        IF (KMTCH(MOM,J).EQ.ITK) MXHT = MXHT + KSMTCH(MOM,J)
        DO 60 I = 1,KNDAU(IMC)
d46 4
a49 2
          DO 60 J = 1,KNMTCH(IDAU)
60        IF (KMTCH(IDAU,J).EQ.ITK) MXHT = MXHT + KSMTCH(IDAU,J)
a55 1
#endif
@


