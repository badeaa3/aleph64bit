@      SUBROUTINE QUIBOS(LOUT,LINTE)
CKEY BANKS INIT /USER
C----------------------------------------------------------------------
C! initialize BOS
C called from QMINIT
C                                                   H.Albrecht 20.09.88
C                                       modified    J.Boucrot  03.05.93
C----------------------------------------------------------------------
#ifndef DOC
      PARAMETER ( MXBNK = 3000 , LQBOS = 5000000 )
      COMMON /BCS/  IW(LQBOS)
C
