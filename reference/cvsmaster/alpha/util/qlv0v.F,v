1.1
log
@*** empty log message ***
@
text
@      SUBROUTINE QLV0V(KFLV0,KLLV0)
CKEY USER VERTEX
C----------------------------------------------------------------------
C!  - Gets vertex numbers for Long V0s for new tracking
C!
C!   Author   :- J. Boucrot                    21-Apr-1997
C!   Output arguments
C!     KFLV0,KLLV0 : first and last Long V0 vertex number
C?
C!======================================================================
#ifndef DOC
#include "qcde.h"
      PARAMETER ( ITYVR = 3 )
#include "qmacro.h"
C----------------------------------------------------------------------
      KFLV0=KFFRV
      KLLV0=KFFRV-1
      KMIN=100000
      KMAX=-1
      IOK=0
      DO 100 IVV=KFREV,KLREV
         IF (KVTYPE(IVV).NE.ITYVR) GO TO 100
         IOK=IOK+1
         KMIN=MIN0(KMIN,IVV)
         KMAX=MAX0(KMAX,IVV)
 100  CONTINUE
      IF (IOK.EQ.0) GO TO 999
      KFLV0=KMIN
      KLLV0=KMAX
C
  999 RETURN
      END
#endif
@
